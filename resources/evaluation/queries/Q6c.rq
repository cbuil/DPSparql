SELECT (COUNT (?humans) as ?count) WHERE {
  ?humans <http://www.wikidata.org/prop/direct/P31> <http://www.wikidata.org/entity/Q5> .         #find humans
  ?humans <http://www.wikidata.org/prop/direct/P2002> ?twittter . #with at least one P40 (child) statement
  ?humans <http://www.wikidata.org/prop/direct/P108> ?organizations .
  ?organizations <http://www.wikidata.org/prop/direct/P31> <http://www.wikidata.org/entity/Q43229> .
  ?organizations <http://www.wikidata.org/prop/direct/P112> ?founded_by
}
