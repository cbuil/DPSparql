PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>

SELECT (COUNT (?human) as ?count) WHERE {
  ?human wdt:P31 wd:Q5 .         #find humans
  ?human wdt:2002 ?twittter . #with at least one P40 (child) statement
  ?human wdt:P108 ?organization .
  ?organization wdt:P31 wd:Q43229 .
  ?organization wdt:P112 ?founded_by
}
